{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/plants.js", "use": "@vercel/node" },
    { "src": "api/categories2.js", "use": "@vercel/node" },
    { "src": "api/updateData.js", "use": "@vercel/node" },
    { "src": "client/package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/static/(.*)", "dest": "/client/build/static/$1" },
    { "src": "/uploads/(.*)", "dest": "/client/build/uploads/$1" },
    { "src": "/favicon.ico", "dest": "/client/build/favicon.ico" },
    { "src": "/logo192.png", "dest": "/client/build/logo192.png" },
    { "src": "/manifest.json", "dest": "/client/build/manifest.json" },
    { "src": "/asset-manifest.json", "dest": "/client/build/asset-manifest.json" },
    { "src": "/(.*)", "dest": "/client/build/index.html" }
  ]
}
